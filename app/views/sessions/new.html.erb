

<main id="sign-in">

  <h5 class="tag-line">A clone of the dating site at www.okcupid.com by Kara Bernert.</h5>

  <form id="sign-in-form" action="<%= session_url %>" method="POST">
	  <%= auth_token %>
    <input type="text" placeholder="Username" name="username">
    <input type="password" placeholder="Password" name="password">
	  <button>SIGN IN</button>
  </form>

  <form id="demo-sign-in" action="<%= session_url %>" method="POST">
	  <%= auth_token %>
    <input type="text" name="username" value="Romeo">
		<input type="text" name="password" value="password">
    <button>SIGN IN AS DEMO USER</button>
  </form>

  <a id="sign-up" href="<%= new_user_url %>">SIGN UP</a>

</main>
